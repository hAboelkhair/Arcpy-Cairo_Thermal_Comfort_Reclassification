##################################################
###############  Directory #######################
mkdir C:/cygwin64/home/dell/CDO-11-Indices-33
export DIR=C:/cygwin64/home/dell/CDO-11-Indices-33
#################################################
########################################################################
cdo selvar,T_MIN_L103_Pd CFSR_Tmin_daily1981-2019.nc CFSR_Tmin_new_daily1981-2019.nc
cdo chname,T_MIN_L103_Pd,t2min CFSR_Tmin_new_daily1981-2019.nc CFSR_Tmin_final_daily1981-2019.nc
cdo selvar,T_MAX_L103_Pd CFSR_Tmax_daily1981-2019.nc CFSR_Tmax_new_daily1981-2019.nc
cdo chname,T_MAX_L103_Pd,t2max CFSR_Tmax_new_daily1981-2019.nc CFSR_Tmax_final_daily1981-2019.nc
################# change unit (kelvin to celsius) #################
cdo addc,-273.15 CFSR_Tmax_final_daily1981-2019.nc CCFSR_Tmax_daily1981-2019.nc
cdo addc,-273.15 Power_Tmax_daily1981-2019.nc CPower_Tmax_daily1981-2019.nc
cdo addc,-273.15 ERA5_Tmax_daily1981-2019.nc CERA5_Tmax_daily1981-2019.nc
cdo addc,-273.15 CFSR_Tmin_final_daily1981-2019.nc CCFSR_Tmin_daily1981-2019.nc
cdo addc,-273.15 Power_Tmin_daily1981-2019.nc CPower_Tmin_daily1981-2019.nc
cdo addc,-273.15 ERA5_Tmin_daily1981-2019.nc CERA5_Tmin_daily1981-2019.nc
########################################################################
################# Monthly maximum value of daily maximum temperature (TXx) #################
cdo yearmax CCFSR_Tmax_daily1981-2019.nc $DIR/CFSR_TXx.nc
cdo yearmax CPower_Tmax_daily1981-2019.nc $DIR/Power_TXx.nc
cdo yearmax CERA5_Tmax_daily1981-2019.nc  $DIR/ERA5_TXx.nc
########################################################################
################# Monthly minimum value of daily maximum temperature ( TXn) ###################
cdo yearmin CCFSR_Tmax_daily1981-2019.nc $DIR/CFSR_TXn.nc
cdo yearmin CPower_Tmax_daily1981-2019.nc $DIR/Power_TXn.nc
cdo yearmin CERA5_Tmax_daily1981-2019.nc  $DIR/ERA5_TXn.nc
########################################################################
################# Monthly maximum value of daily minimum temperature (TNx) ################
cdo yearmax CCFSR_Tmin_daily1981-2019.nc $DIR/CFSR_TNx.nc
cdo yearmax CPower_Tmin_daily1981-2019.nc $DIR/Power_TNx.nc
cdo yearmax CERA5_Tmin_daily1981-2019.nc  $DIR/ERA5__TNx.nc
########################################################################
################# Monthly minimum value of daily minimum temperature (TNn)  ################
cdo yearmin CCFSR_Tmin_daily1981-2019.nc $DIR/CFSR_TNn.nc
cdo yearmin CPower_Tmin_daily1981-2019.nc $DIR/Power_TNn.nc
cdo yearmin CERA5_Tmin_daily1981-2019.nc  $DIR/ERA5__TNn.nc
########################################################################
